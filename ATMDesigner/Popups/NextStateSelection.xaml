<Window x:Class="ATMDesigner.UI.Popups.NextStateSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         WindowStartupLocation="CenterScreen" 
        Closing="NextStateSelection_Closing"
        Title="Choose Next State" Height="150.4" Width="250.8" ResizeMode="NoResize">
    <Grid>
        <GroupBox Header="Next State Selection" ></GroupBox>
        
       

        <WrapPanel Grid.Row="1" Grid.Column="3" Margin="10,30,0,10" HorizontalAlignment="Left">
            <ComboBox Name="CmbState" IsTextSearchEnabled="True"  Width="220">
              
            </ComboBox>            
        </WrapPanel>

        <WrapPanel HorizontalAlignment="Center" Margin="10,75,0,10">
            <Button IsDefault="True" Name="btnDialogOk" Click="btnDialogOk_Click"  MinWidth="60" Margin="0,0,10,0" Height="28.36">_Ok</Button>
            <Button IsCancel="True" Click="btnDialogCancel_Click" MinWidth="60">_Cancel</Button>
        </WrapPanel>
      

    </Grid>
</Window>
