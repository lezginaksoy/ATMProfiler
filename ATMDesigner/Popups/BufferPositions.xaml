<Window x:Class="ATMDesigner.UI.Popups.BufferPositions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         WindowStartupLocation="CenterScreen" 
        Title="Set BufferPositions" Height="150.4" Width="250.8" ResizeMode="NoResize">
    <Grid>
        <GroupBox Header="BufferPositions" ></GroupBox>
        
         <WrapPanel Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" Margin="0,35,10,0">
            <ComboBox Name="CmbLast" IsTextSearchEnabled="True"  Width="50">
            </ComboBox>
        </WrapPanel>

        <WrapPanel Grid.Row="1" Grid.Column="2" Margin="0,35,0,0" HorizontalAlignment="Center">
            <ComboBox Name="CmbMedium" IsTextSearchEnabled="True"  Width="50">
            </ComboBox>
        </WrapPanel>

        <WrapPanel Grid.Row="1" Grid.Column="3" Margin="10,35,0,0" HorizontalAlignment="Left">
            <ComboBox Name="Cmbfirst" IsTextSearchEnabled="True"  Width="50">
            </ComboBox>
        </WrapPanel>

        <WrapPanel HorizontalAlignment="Center" Margin="100,80,0,0">
            <Button IsDefault="True" Name="btnDialogOk" Click="btnDialogOk_Click"  MinWidth="60" Margin="0,0,10,0" Height="28.36">_Ok</Button>
            <Button IsCancel="True" MinWidth="60">_Cancel</Button>
        </WrapPanel>
      

    </Grid>
</Window>
